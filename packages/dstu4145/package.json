{
  "name": "@1-corp/dstu4145",
  "version": "0.0.1",
  "description": "Ukrainian national EC cryptography (DSTU-4145) TypeScript only implementaion",
  "main": "lib/index.js",
  "scripts": {
    "audit": "yarn audit",
    "test": "jest",
    "check": "gts check",
    "clean": "gts clean",
    "fix": "gts fix",
    "lint": "eslint .",
    "build": "babel src --source-maps --out-dir lib",
    "build:watch": "babel src --watch --source-maps --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:1-corp/ts-crypto-ua"
  },
  "keywords": [
    "ecc",
    "cryptography",
    "dstu4145",
    "ukraine",
    "typescript"
  ],
  "author": "Bohdan Vanieiev",
  "contributors": [
    "Ilya Petrov"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1-corp/ts-crypto-ua/issues"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "test"
    ],
    "rootDir": ".",
    "transformIgnorePatterns": [
      "node_modules/.*",
      ".*\\.js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "verbose": true,
    "testRegex": ".(j|t)s$",
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "homepage": "https://github.com/1-corp/ts-crypto-ua/packages/dstu4145",
  "dependencies": {
    "@1-corp/dstu-gost": "file:../dstu-gost",
    "@1-corp/jksreader": "file:../jksreader",
    "asn1.js": ">=5.3.0",
    "bn.js": ">=5.1",
    "buffer": ">=5.5",
    "js-lzma": "0.x"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-es6-promise": "1.1.1",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "eslint": "3.19.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "6.10.3",
    "eslint-plugin-standard": "3.0.1"
  }
}